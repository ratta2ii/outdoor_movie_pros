{
    "hosting": {
        "public": "build",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "function": "handleRequests"
            }
        ],
        "redirects": [
            {
                "source": "/index.html",
                "destination": "/",
                "type": 301
            },
            {
                "source": "https://www.outdoormoviepros.com/**",
                "destination": "https://outdoormoviepros.com",
                "type": 301
            }
        ],
        "headers": [
            {
                "source": "**/*.@(js|css|jpg|jpeg|gif|png|svg|ico)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "max-age=31536000"
                    }
                ]
            },
            {
                "source": "**",
                "headers": [
                    {
                        "key": "X-Prerender-Token",
                        "value": "TODO:ADD_NEW_TOKEN_HERE"
                    }
                ]
            }
        ]
    },
    "functions": {
        "source": "functions",
        "runtime": "nodejs20"
    }
}